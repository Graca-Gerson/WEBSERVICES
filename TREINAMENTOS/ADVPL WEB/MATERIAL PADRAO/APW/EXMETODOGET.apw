#include "Protheus.ch"
#INCLUDE 'APWEBEX.CH' 

USER FUNCTION EXMETODOGET
Local CHTML := ""

//CHAMA A MESMA FUNÇÃO - SÓ QUE O APH (UM DEPENDE DO OUTRO) -- O APH ESTÁ CHAMANDO ABAIXO
WEB EXTENDED INIT CHTML

//EXEMPLO DO METODO POST

	IF !EMPTY(HTTPGET->VAR1)
	
		//EXIBE NO CONSOLE DO SERVER OS DADOS DAS VARIAVEIS
		CONOUT(HTTPGET->VAR1)
		CONOUT(HTTPGET->VAR2)
	ENDIF

//NOME DO ARQUIVO APH PARA EXIBIÇÃO
CHTML := EXECINPAGE("METODOGET")

WEB EXTENDED END

RETURN(CHTML)